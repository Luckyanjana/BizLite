
<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Bizlite | @ViewBag.Title</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="../assets/img/icon.ico" type="image/x-icon" />
    <!-- Fonts and icons -->
    <script src="~/js/plugin/webfont.min.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Lato:300,400,700,900"] },
            custom: { "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands", "simple-line-icons"], urls: ['../css/fonts.min.css'] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

    <!-- CSS Files -->
    <link href="~/js/plugin/bootstrap.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/js/plugin/atlantis.min.css">
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link rel="stylesheet" href="~/js/plugin/demo.css">
</head>
<!--
BODY TAG OPTIONS:
=================
Apply one or more of the following classes to to the body tag
to get the desired effect
|---------------------------------------------------------|
|LAYOUT OPTIONS | sidebar-collapse                        |
|               | sidebar-mini                            |
|---------------------------------------------------------|
-->
<body>

    <div class="wrapper">
        <partial name="_Header.cshtml" />
        <partial name="_Sidebar.cshtml" />
        <!-- Content Wrapper. Contains page content -->
        <div class="main-panel">
            <div class="content">
                <div class="page-inner">
                    @RenderBody()
                </div>
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->


        <partial name="_Footer.cshtml" />
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/js/plugin/jquery.3.2.1.min.js"></script>
    <script src="~/js/plugin/popper.min.js"></script>
    <script src="~/js/plugin/bootstrap.min.js"></script>

    <!-- jQuery UI -->
    <script src="~/js/plugin/jquery-ui.min.js"></script>


    <script src="~/js/plugin/jquery.ui.touch-punch.min.js"></script>
    <!-- jQuery Scrollbar -->
    <script src="~/js/plugin/jquery.scrollbar.min.js"></script>
    <!-- jQuery Sparkline -->
    <script src="~/js/plugin/jquery.sparkline.min.js"></script>
    <!-- Atlantis JS -->
    <script src="~/js/atlantis.min.js"></script>
    <script src="~/js/setting-demo.js"></script>
    <!-- Atlantis DEMO methods, don't include it in your project! -->
    <!-- OPTIONAL SCRIPTS -->
    @await RenderSectionAsync("Scripts", required: false)<!--End area -->


</body>
</html>
